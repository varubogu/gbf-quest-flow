{
  "name": "eminent-ellipse",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev --host",
    "build": "astro build",
    "deploy": "bun install && bunx --bun astro build && mkdir -p dist/content && cp -r src/content/flows dist/content/flows",
    "preview": "astro preview --host",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.astro",
    "postbuild": "mkdir -p dist/content && cp -r src/content/flows dist/content/flows"
  },
  "dependencies": {
    "@astrojs/react": "^4.2.0",
    "@astrojs/tailwind": "^6.0.0",
    "@radix-ui/react-slot": "^1.1.2",
    "@shadcn/ui": "^0.0.4",
    "@tailwindcss/postcss": "^4.0.5",
    "@tailwindcss/vite": "^4.0.5",
    "astro": "^5.2.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.475.0",
    "postcss": "^8.5.1",
    "react-resizable-panels": "^2.1.7",
    "tailwind-merge": "^3.0.1",
    "tailwindcss": "^4.0.5",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "eslint": "^9.20.0",
    "eslint-plugin-astro": "^1.3.1",
    "prettier": "^3.5.0",
    "prettier-plugin-astro": "^0.14.1"
  }
}