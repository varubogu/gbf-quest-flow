---
// Reactコンポーネントとして描画
import FlowBodyLayout from "@/layouts/FlowBodyLayout";
import sampleDataJson from "@/content/sample_data.json"; // JSONファイルをインポート
import type { Flow } from "@/types/models";
import FlowBaseLayout from "@/layouts/FlowBaseLayout.astro";

// structuredCloneを使用してデータをクローン
const data: Flow = sampleDataJson;
// Base64エンコードしてデータを渡す
const encodedData = btoa(encodeURIComponent(JSON.stringify(data)));
---

<FlowBaseLayout data={data} client:load>
  <FlowBodyLayout data={data} client:only="react" />
</FlowBaseLayout>