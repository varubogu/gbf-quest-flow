---
// Astroコンポーネントとして描画
import FlowBaseLayout from "@/layouts/FlowBaseLayout.astro";
import FlowBodyLayout from "@/layouts/FlowBodyLayout.astro";
import sampleDataJson from "@/content/sample_data.json"; // JSONファイルをインポート
import type { Flow } from "@/types/models";

const data: Flow = sampleDataJson;
const title = data.title;
---

<FlowBaseLayout title={title} client:load>
  <FlowBodyLayout data={data} client:load />
</FlowBaseLayout>
